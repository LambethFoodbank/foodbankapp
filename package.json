{
  "name": "norwood-and-brixton-foodbank",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:test_server": "next start -p 3200",
    "typecheck": "tsc",
    "lint": "npm run typecheck && next lint --dir . --ignore-path .gitignore",
    "lint_fix": "npm run typecheck && next lint --fix --dir . --ignore-path .gitignore",
    "lint:ci": "next lint --dir . --ignore-path .gitignore && npm run typecheck",
    "test": "npm run jest:all && npm run cypress:all",
    "test:component": "npm run cypress:component && npm run jest:component",
    "test:e2e": "cypress:e2e",
    "test:coverage": "npm run coverage:generate && npm run coverage:run_tests || npm run coverage:report",
    "cypress:all": "npm run cypress:component && npm run cypress:e2e",
    "cypress:component": "",
    "cypress:e2e": "start-server-and-test start:test_server 3200 'npx cypress run --e2e'",
    "cypress:open": "start-server-and-test 'next start -p 3100' 3100 'npx cypress open --config baseUrl=http://localhost:3100'",
    "jest:all": "npm run jest:component",
    "jest:component": "npx jest",
    "coverage:generate": "nyc instrument --delete src .nyc_output --compact=false --all",
    "coverage:run_tests": "start-server-and-test 'nyc npm run start:test_server' 3200 cypress:e2e",
    "coverage:report": "nyc report --reporter text && nyc report --reporter text-summary",
    "reset_supabase": "supabase db reset --local && ts-node supabase/tools/createUserForEachRole.ts && ts-node supabase/tools/createStorage.ts",
    "generate_types:local": "supabase gen types typescript --local --schema public > src/databaseTypesFile.ts",
    "generate_types:remote": "supabase gen types typescript --schema public > src/databaseTypesFile.ts",
    "db:generate_seed": "npx ts-node supabase/seed/seed.ts > supabase/seed.sql",
    "post_checkout": "npm install && npx supabase stop && npx supabase start && npm run reset_supabase && npx snaplet generate"
  },
  "nyc": {
    "all": true,
    "extends": "@istanbuljs/nyc-config-typescript",
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/**/*.cy.ts",
      "src/**/*.cy.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ]
  },
  "browserslist": [
    "defaults",
    "Safari >= 13.1.2",
    "Chrome >= 116"
  ],
  "dependencies": {
    "@emotion/is-prop-valid": "^1.2.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.0.5",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@fullcalendar/core": "^6.1.8",
    "@fullcalendar/daygrid": "^6.1.8",
    "@fullcalendar/interaction": "^6.1.8",
    "@fullcalendar/react": "^6.1.8",
    "@fullcalendar/timegrid": "^6.1.8",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.15.14",
    "@mui/x-data-grid": "^6.19.6",
    "@mui/x-date-pickers": "^6.10.2",
    "@react-pdf/renderer": "^3.1.12",
    "@supabase/auth-helpers-nextjs": "^0.7.2",
    "@supabase/gotrue-js": "^2.31.0",
    "@supabase/supabase-js": "^2.26.0",
    "@types/file-saver": "^2.0.7",
    "@types/node": "20.3.1",
    "@types/pdf-parse": "^1.1.1",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/react-fontawesome": "^1.6.8",
    "@types/react-modal": "^3.16.3",
    "@types/uuid": "^9.0.8",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "encoding": "^0.1.13",
    "file-saver": "^2.0.5",
    "next": "^14.2.15",
    "nyc": "^15.1.0",
    "pdf-parse": "^1.1.1",
    "react": "^18.3.1",
    "react-data-table-component": "^7.5.4",
    "react-dom": "^18.3.1",
    "react-modal": "^3.16.1",
    "start-server-and-test": "^2.0.0",
    "styled-components": "^6.0.4",
    "supabase": "^1.142.2",
    "typescript": "^5.5.3",
    "uuid": "^9.0.1",
    "winston": "^3.12.0",
    "winston-cloudwatch": "^6.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/types": "^7.24.9",
    "@cypress/code-coverage": "^3.11.0",
    "@jest/globals": "^29.7.0",
    "@next/bundle-analyzer": "^14.2.5",
    "@snaplet/copycat": "^5.0.0",
    "@snaplet/seed": "^0.88.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/cypress-dotenv": "^2.0.1",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^29.5.12",
    "@types/react-test-renderer": "^18.3.0",
    "@types/seedrandom": "^3.0.8",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "axe-core": "^4.7.2",
    "babel-jest": "^29.7.0",
    "cypress": "^13.15.0",
    "cypress-axe": "^1.5.0",
    "eslint": "^8.43.0",
    "eslint-config-next": "^14.2.15",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transformer-svg": "^2.0.2",
    "react-router-dom": "^6.25.1",
    "react-test-renderer": "^18.3.1",
    "seedrandom": "^3.0.5",
    "snaplet": "^0.88.2",
    "text-encoding": "^0.7.0",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2"
  }
}

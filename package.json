{
  "name": "vauxhall-foodbank",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "npm run build && next start",
    "lint": "next lint --dir src --dir cypress",
    "lint_fix": "next lint --fix --dir src --dir cypress",
    "test": "start-server-and-test dev http://localhost:3000 test:runner",
    "test:component": "start-server-and-test dev http://localhost:3000 test:runner_component",
    "test:e2e": "start-server-and-test dev http://localhost:3000 test:runner_e2e",
    "test:coverage": "npm run coverage:generate && npm run coverage:run_tests && npm run coverage:report",
    "test:runner": "npm run test:runner_component && npm run test:runner_e2e",
    "test:runner_component": "npx cypress run --component",
    "test:runner_e2e": "npx cypress run",
    "coverage:generate": "nyc instrument --delete src .nyc_output --compact=false --all",
    "coverage:run_tests": "start-server-and-test coverage:server http://localhost:3000 test:runner",
    "coverage:server": "nyc npm run dev",
    "coverage:report": "nyc report --reporter html && nyc report --reporter text-summary"
  },
  "nyc": {
    "all": true,
    "extends": "@istanbuljs/nyc-config-typescript"
  },
  "dependencies": {
    "@cypress/code-coverage": "^3.10.7",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.5",
    "@emotion/is-prop-valid": "^1.2.1",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@fullcalendar/core": "^6.1.8",
    "@fullcalendar/daygrid": "^6.1.8",
    "@fullcalendar/interaction": "^6.1.8",
    "@fullcalendar/react": "^6.1.8",
    "@fullcalendar/timegrid": "^6.1.8",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@supabase/auth-helpers-nextjs": "^0.7.2",
    "@supabase/auth-ui-react": "^0.4.2",
    "@supabase/auth-ui-shared": "^0.1.6",
    "@supabase/gotrue-js": "^2.31.0",
    "@supabase/supabase-js": "^2.26.0",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/cypress-dotenv": "^2.0.0",
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "@types/react-fontawesome": "^1.6.5",
    "@types/react-modal": "^3.16.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "axe-core": "^4.7.2",
    "cypress": "^12.15.0",
    "cypress-axe": "^1.4.0",
    "dotenv": "^16.3.1",
    "encoding": "^0.1.13",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.6",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "next": "^13.4.7",
    "nyc": "^15.1.0",
    "react": "18.2.0",
    "react-data-table-component": "^7.5.3",
    "react-dom": "18.2.0",
    "react-modal": "^3.16.1",
    "start-server-and-test": "^2.0.0",
    "styled-components": "^6.0.0-rc.3",
    "supabase": "^1.68.6",
    "typescript": "5.0.4"
  }
}
